<div class="{{inputRowCLasses}}">
    <label class="{{labelClasses}}" for="{{key}}">{{label}}</label>

    <label class="{{inputContainerClasses}} custom-switch mt-2" for="{{key}}">
        <input type="checkbox" name="{{key}}" class="{{inputClasses}} custom-switch-input" id="{{key}}" {{options}}>
        <span class="custom-switch-indicator"></span>
    </label>
</div>
