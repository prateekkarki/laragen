<div class="form-group row">
    <label class="col-md-2 form-control-label" for="{{key}}">{{display}}</label>
    <div class="col-md-10">
        <input class="form-control datepicker" type="text" name="{{key}}" id="{{key}}" placeholder="{{display}}" value="{{ isset(${{modelNameLowercase}}) ? ${{modelNameLowercase}}->{{key}} : '' }}" {{options}}>
    </div>
</div>
