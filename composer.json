{
    "name": "prateekkarki/laragen",
    "type": "library",
    "description": "Extended generators for laravel",
    "keywords": [
        "prateekkarki",
        "laragen"
    ],
    "homepage": "https://github.com/prateekkarki/laragen",
    "license": "MIT",
    "authors": [
        {
            "name": "Prateek Karki",
            "email": "pratiek.karki@gmail.com",
            "homepage": "http://prateekkarki.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "symfony/filesystem" : ">=4.0",
        "laravel/framework": ">=5.7",
        "intervention/image" : ">=2.0"
    },
    "autoload": {
        "psr-4": {
            "Prateekkarki\\Laragen\\": "src"
        }
    },
    "scripts": {
        "pre-laragen-init": [
            "Prateekkarki\\Laragen\\Initialize::initSetup"
        ],
        "pre-install-cmd": "Prateekkarki\\Laragen\\Initialize::preInstall",
        "post-install-cmd": [
            "Prateekkarki\\Laragen\\Initialize::postInstall"
        ],
        "post-package-install": [
            "Prateekkarki\\Laragen\\Initialize::postPackageInstall"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Prateekkarki\\Laragen\\LaragenServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
